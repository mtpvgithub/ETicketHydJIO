<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg" >

    <RelativeLayout
        android:id="@+id/relativeLayout1"
        style="@style/header_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" >

        <ImageView
            android:id="@+id/imageView1"
            style="@style/header_images_style_left"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true" />

        <ImageView
            android:id="@+id/imageView2"
            style="@style/header_images_style_right"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true" />

        <TextView
            android:id="@+id/textView1"
            style="@style/header_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/header_txt" />

        <TextView
            android:id="@+id/textView2"
            style="@style/cust_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/imageView2"
            android:layout_centerHorizontal="true"
            android:text="@string/app_version"
            android:textAppearance="?android:attr/textAppearanceSmall" />
    </RelativeLayout>

    <ImageButton
        android:id="@+id/imgbtn_logout_dashboard_xml"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_below="@+id/relativeLayout1"
        android:layout_marginBottom="0dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="10dp"
        android:background="@null"
        android:src="@drawable/logout" />

    <TextView
        android:id="@+id/tv_login_username_dashboard_xml"
        style="@style/navi_text_style"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/imgbtn_logout_dashboard_xml"
        android:layout_marginLeft="10dp"
        android:text="Welcome" />

    <LinearLayout
        android:id="@+id/ll_dashbrd1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imgbtn_logout_dashboard_xml"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="35dp" >
         <!-- android:layout_margin="@dimen/header_grnt_padding_ten"  -->

        <TextView
            android:id="@+id/tv_generateticket_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/generate_ticket"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/generate_ticket" />

        <TextView
            android:id="@+id/tv_spot_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/spot_challan"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/spot_challan" />

        <TextView
            android:id="@+id/tv_vhclehistory_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/vhcle_history"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/vehicle_history" />
    </LinearLayout>

    <TextView
        android:id="@+id/textView3"
        style="@style/footer_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />

    <LinearLayout
        android:id="@+id/linearLayout1"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ll_dashbrd1"
        android:layout_centerHorizontal="true" >

        <TextView
            android:id="@+id/tv_towing_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/towing"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/towing" />

        <TextView
            android:id="@+id/tv_releasedocuments_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/releasedocument"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/release_documents" />

        <TextView
            android:id="@+id/tv_reports_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/reports"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/reports" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout3"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout1"
        android:layout_centerHorizontal="true" >

        <TextView
            android:id="@+id/tvduplicateprint_dashboad_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:background="@null"
            android:drawableTop="@drawable/print"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/duplicate_print" />

        <TextView
            android:id="@+id/tv_sync_dashboard_xml"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:background="@null"
            android:drawableTop="@drawable/sync"
            android:gravity="center_horizontal|center_vertical"
            android:text="@string/sync" />
        
            
              <TextView
             android:id="@+id/tv_settings_dashboard_xml"
             style="@style/navi_text_style"
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:layout_weight="1"
             android:background="@null"
             android:drawableTop="@drawable/settings"
             android:gravity="center_horizontal|center_vertical"
             android:text="@string/settings" />

    </LinearLayout>
    
    <LinearLayout
        android:id="@+id/linearLayout4"
        style="@style/margin_top_btm_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linearLayout3"
        android:gravity="center"
        android:visibility="gone"
        android:layout_centerHorizontal="true" >

       <TextView
            android:id="@+id/tv_special_drive"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="01"
            android:background="@null"
            android:visibility="gone"
            android:drawableTop="@drawable/sp_drive"
            android:layout_below="@+id/linearLayout3"
            android:gravity="center"
            android:layout_centerInParent="true"
            android:text="@string/sp_drive" />

        <TextView
            android:id="@+id/echallan"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0"
            android:background="@null"
            android:drawableTop="@drawable/spot_challan"
            android:gravity="center_horizontal"
            android:text="@string/echallan" />
        
        <TextView
            android:id="@+id/echallan_reports"
            style="@style/navi_text_style"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:background="@null"
            android:drawableTop="@drawable/spot_challan"
            android:gravity="center"
            android:text="@string/echallanreports" />

    </LinearLayout>
    
